<?xml version="1.0" encoding="utf-8"?>
<resources>
    <color name="colorPrimary">#3F51B5</color>
    <color name="colorPrimaryDark">#303F9F</color>
    <color name="colorAccent">#FF4081</color>
    <color name="colorGreen">#06bd06</color>
    <color name="colorTranslucentGreen">#8006bd06</color>
    <color name="color_clicked_gray">#30aaaaaa</color>
    <color name="colorTitleBar">#2faf22</color>
    <color name="colorClickedBack">#0e8c01</color>
    <color name="colorPrimaryNetState">#90ffbcba</color>,
    <color name="colorPressedNetState">#fcadab</color>
    <color name="colorPopupWindow">#a0000000</color>
    <color name="colorTimeRound">#61000000</color>
    <color name="color_background">#20aaaaaa</color>
    <color name="color_divider">#40aaaaaa</color>

    <!-- ========================== 自定义 PhotoPicker 颜色 START ========================== -->
    <!-- PhotoPicker 状态栏的颜色 -->
    <color name="bga_pp_colorPrimaryDark">@android:color/black</color>
    <!-- PhotoPicker 导航栏的颜色 -->
    <color name="bga_pp_navigationBarColor">@android:color/black</color>
    <!-- PhotoPicker Toolbar 的颜色 -->
    <color name="bga_pp_colorPrimary">@android:color/black</color>
    <!-- 图片选择器库里所有Activity界面的背景色 -->
    <color name="bga_pp_activity_bg">@android:color/black</color>
    <!-- 文字颜色 -->
    <color name="bga_pp_common_textColor">@android:color/white</color>
    <!-- 文件夹名称颜色 -->
    <color name="bga_pp_folder_name_textColor">#282828</color>
    <!-- 文件夹中照片数量颜色 -->
    <color name="bga_pp_folder_count_textColor">#585858</color>
    <!-- 图片条目背景色 -->
    <color name="bga_pp_photo_item_bg">#e2e2e2</color>
    <!-- 拍摄照片文字颜色 -->
    <color name="bga_pp_take_photo_textColor">#93979f</color>
    <!-- 图片处于选中状态时的遮罩层颜色 -->
    <color name="bga_pp_photo_selected_mask">#4DFFFFFF</color>
    <!-- 右上角确认按钮不可用时的颜色 -->
    <color name="bga_pp_btn_confirm_disabled">@android:color/transparent</color>
    <!-- 右上角确认按钮可用时的颜色 -->
    <color name="bga_pp_btn_confirm_enabled">@android:color/transparent</color>
    <!-- 右上角确认按钮按下时的颜色 -->
    <color name="bga_pp_btn_confirm_pressed">@android:color/transparent</color>
    <!-- 预览界面底部背景色 -->
    <color name="bga_pp_preview_bottom_bg">@android:color/black</color>
    <!-- 数据加载对话框加载进度条的颜色 -->
    <color name="bga_pp_loading_progress_startColor">@android:color/white</color>
    <color name="bga_pp_loading_progress_centerColor">@color/colorPrimaryDark</color>
    <!--<color name="bga_pp_loading_progress_endColor">@color/colorPrimaryDarkTrans</color>-->
